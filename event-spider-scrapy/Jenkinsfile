pipeline {
    agent any
    stages {
        stage('install') {
            steps {
            	echo "install scrapy"
                sh 'pip install scrapy'
            }
        }
        stage('checkout') {
            steps {
            	echo "checkout scm"
                checkout scm
            }
        }
        stage('run spider') {
        	steps {
        		echo "run spider"
        	}
        }
    }
}